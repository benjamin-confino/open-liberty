-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

openTelemetryVersion=1.19.0

Bundle-Name: io.openliberty.osgi.internal.common
Bundle-SymbolicName: io.openliberty.osgi.internal.common
Bundle-Description: OSGi Helper Classes

javac.source: 11
javac.target: 11

Require-Capability: osgi.ee; filter:="(&(osgi.ee=JavaSE)(version=11))"

src: src, resources

-includeresource: resources

Import-Package: \
	*

Export-Package: \
	io.openliberty.osgi.internal.common;version='1.0'

Private-Package: io.openliberty.microprofile.telemetry10.internal.helper,\
	io.openliberty.microprofile.telemetry10.internal.sdk

-buildpath: \
	com.ibm.websphere.appserver.spi.logging,\
	com.ibm.wsspi.org.osgi.service.component.annotations;version=latest,\
	com.ibm.websphere.org.osgi.core,\
	com.ibm.websphere.org.osgi.service.component,\
	com.ibm.ws.kernel.service;version=latest,\
        com.ibm.ws.org.osgi.annotation.versioning;version=latest
